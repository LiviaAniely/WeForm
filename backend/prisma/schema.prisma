generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CampoDeAtuacao {
  id        Int        @id @default(autoincrement())
  campo     String
  entidades Entidade[]
}

model Entidade {
  id                 Int    @id @default(autoincrement())
  nome               String
  campoDeColeta      String
  criadaEm           DateTime @default(now())
  campoDeAtuacaoId   Int?
  campoDeAtuacao     CampoDeAtuacao? @relation(fields: [campoDeAtuacaoId], references: [id])
}